{
  "name": "graphql-typescript-webpack-starter",
  "version": "1.0.0",
  "description": "A starter template for GraphQL in Typescript build with Webpack.",
  "main": "index.js",
  "scripts": {
    "test": "jest --forceExit",
    "test:watch": "jest --watch",
    "test:notify": "npm run test:watch -- --notify",
    "coverage": "npm test -- --coverage",
    "watch": "webpack --watch",
    "build": "webpack",
    "nodemon": "nodemon dist/server.js",
    "lint": "tslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "concurrently \"npm run watch\" \"npm run nodemon\"",
    "start": "npm run build && node dist/server.js"
  },
  "keywords": [
    "Graphql",
    "Webpack",
    "Typescript",
    "Node",
    "Express",
    "Apollo"
  ],
  "author": "Christian LÃ¼demann",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "1.3.2",
    "body-parser": "1.18.2",
    "cors": "2.8.4",
    "dotenv": "4.0.0",
    "express": "4.16.2",
    "graphql": "0.11.7",
    "graphql-tools": "2.18.0",
    "inversify": "3.1.0",
    "json-to-graphql": "0.1.1",
    "node-fetch": "1.7.3",
    "reflect-metadata": "0.1.10",
    "tslib": "1.8.1"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.8",
    "@types/express": "4.11.0",
    "@types/graphql": "0.11.7",
    "@types/jest": "^22.0.1",
    "@types/node": "9.3.0",
    "@types/node-fetch": "1.6.7",
    "apollo-link": "1.0.7",
    "awesome-typescript-loader": "3.4.1",
    "concurrently": "3.5.1",
    "graphql-tag": "^2.6.1",
    "jest": "^22.1.0",
    "jest-cli": "^22.1.0",
    "jest-transform-graphql": "^2.1.0",
    "nodemon": "1.14.10",
    "ts-jest": "^22.0.1",
    "ts-transform-graphql-tag": "^0.1.2",
    "tslint": "5.9.1",
    "tslint-loader": "^3.5.3",
    "typescript": "2.6.2",
    "webpack": "3.10.0",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "testEnvironment": "node",
    "testRegex": ".*\\entry\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/index.ts",
      "!src/interfaces/**/*.ts"
    ]
  }
}
